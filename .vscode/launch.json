{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üêç Debug FastAPI (Uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "src.manage:app",
        "--reload",
        "--host",
        "127.0.0.1",
        "--port",
        "8088"
      ],
      "jinja": true,
      "justMyCode": true,
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "cwd": "${workspaceFolder}"
    }
  ]
}